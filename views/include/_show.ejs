<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].nome %></td>
    <td><%= users[i].sobrenome %></td>
    <td><%= users[i].empresa %></td>
    <td><%= users[i].cargo %></td>
    <td><%= users[i].apelido %></td>
    <td><%= users[i].data_aniversario %></td>
    <td><%= users[i].emails %></td>
    <td><%= users[i].telefones %></td>
    <td><%= users[i].enderecos %></td>
    <td><%= users[i].observacoes %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>